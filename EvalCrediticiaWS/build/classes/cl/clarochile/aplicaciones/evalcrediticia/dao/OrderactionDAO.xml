<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.amx.service.dao.OrderactionDAO">

	<resultMap id="listaDatosMap" type="orderactionTo">
		<result property="corrProduct" column="CORR_PRODUCTODIFINFO" />
		<result property="corrContactinfo" column="CORR_CONTACTINFO" />
		<result property="orderActionid" column="ORDER_ACTIONID" />
		<result property="orderActiontype" column="ORDER_ACTIONTYPE" />
		<result property="oaSrd" column="OA_SRD" />
		<result property="usuario" column="CORR_USUARIOALTA" />

 
	</resultMap>
	<parameterMap id="OrderActionParam" type="orderactionTo">
		<parameter property="resultados" jdbcType="CURSOR" javaType="java.sql.ResultSet" mode="OUT" resultMap="listaDatosMap" />
	</parameterMap>
	<select id="buscarOrder"  parameterType="orderactionTo" statementType="CALLABLE" >
		{ call  pro_evc_buscarORDERACTION(
			#{corrProduct,mode=IN,jdbcType=NUMERIC},
			#{resultados,mode=OUT,jdbcType=CURSOR,
			javaType=java.sql.ResultSet,resultMap=listaDatosMap}
			)
		}

	</select>
	<select id="ingresarOrder" parameterType="orderactionTo"
		statementType="CALLABLE" >
		{ call  pro_evc_ingresarORDERACTION(
		#{corrProduct,mode=IN,jdbcType=NUMERIC}, 
		#{corrContactinfo,mode=IN, jdbcType=VARCHAR},
		#{orderActionid,mode=IN, jdbcType=VARCHAR},
		#{orderActiontype,mode=IN,jdbcType=VARCHAR},
		#{oaSrd,mode=IN,jdbcType=DATE},
		#{usuario,mode=IN,jdbcType=NUMERIC})
		}

	</select>
	
</mapper>