<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.amx.service.dao.ConditionFCEDAO">
                                                           
    <resultMap id="resultConditionFCE" type="conditionFCETO">
        <result property="codCondicionFCE" column="CORR_CONDITIONFCE"/>
        <result property="codSalidaFCE" column="CORR_SALFCE"/>
        <result property="codOrdenActionInfo" column="CORR_ORDERACTIONINFO"/>
        <result property="mesesAdelanto" column="NUM_RCINADVANCED"/>
        <result property="instZero" column="INST_ZERO"/>
        <result property="cantCuotas" column="NUM_INSTALLMENT"/>
        <result property="amount" column="INST_AMOUNT"/>
        <result property="deposit" column="DEPOSIT"/>
        <result property="codeUsuario" column="CORR_USUARIOALTA"/>

    </resultMap>

    <parameterMap id="ConditionFCEParam" type="conditionFCETO">  
        <parameter property="resultados" jdbcType="CURSOR" javaType="java.sql.ResultSet" mode="OUT" resultMap="resultConditionFCE" />
    </parameterMap>
    
    <select id="listarConditionFCEById" parameterMap="ConditionFCEParam" resultMap="resultConditionFCE" parameterType="conditionFCETO" statementType="CALLABLE" >
         { call  PRO_EVC_BUSCARINGSALCONDFCE
            (
               #{codCondicionFCE, mode=IN, jdbcType=NUMERIC},               
               #{resultados, jdbcType=CURSOR, mode=OUT, resultMap=resultConditionFCE, javaType=java.sql.ResultSet}
            )
         }
    </select>
    

      
    <insert id="insertarConditionFCE" parameterType="conditionFCETO" statementType="CALLABLE">
        { call  PRO_EVC_INGSALCONDFCE
            (
               #{codSalidaFCE, mode=IN, jdbcType=NUMERIC},
               #{codOrdenActionInfo, mode=IN, jdbcType=NUMERIC},
               #{mesesAdelanto, mode=IN, jdbcType=VARCHAR},            
               #{instZero, mode=IN, jdbcType=VARCHAR},            
               #{cantCuotas, mode=IN, jdbcType=VARCHAR},            
               #{amount, mode=IN, jdbcType=VARCHAR},            
               #{deposit, mode=IN, jdbcType=VARCHAR},            
               #{codeUsuario, mode=IN, jdbcType=VARCHAR}            

            )
        }
    </insert>
    
</mapper>
