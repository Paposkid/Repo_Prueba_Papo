<?xml version='1.0' encoding='UTF-8'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" id="WebApp_ID">
  <display-name>EvalCrediticiaWS</display-name>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>/WEB-INF/application-context-dao.xml
				/WEB-INF/application-context-ds.xml
				/WEB-INF/application-context-ws.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>PerformFullCreditEvaluationServiceServlethttp</servlet-name>
    <servlet-class>cl.clarochile.aplicaciones.evalcrediticia.performfullcreditevaluation.servicio.PerformFullCreditEvaluationService_PerformFullCreditEvaluationPortImpl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>PerformFullCreditEvaluationServiceServlethttp</servlet-name>
    <url-pattern>PerformFullCreditEvaluationService</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>
